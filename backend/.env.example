# Exemple de configuration environnement pour le backend G-House
# Dupliquer ce fichier en .env et renseigner les valeurs réelles

PORT=5000
MONGODB_URI=mongodb+srv://user:password@cluster-url/dbname
JWT_SECRET=remplacez_par_un_secret_solide

# Stripe
STRIPE_SECRET_KEY=sk_test_...
STRIPE_WEBHOOK_SECRET=whsec_...

# Frontend (URL déployée)
VERCEL_FRONTEND_URL=https://g-house.vercel.app

# Cloudinary
CLOUDINARY_CLOUD_NAME=xxxxx
CLOUDINARY_API_KEY=xxxxxxxxxxxxxxxx
CLOUDINARY_API_SECRET=xxxxxxxxxxxxxxxxxxxxxxxx
